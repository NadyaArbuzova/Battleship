<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.battleship.Statistics">
   <children>
      <TableView fx:id="table" layoutY="-1.0" prefHeight="249.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="300.0" text="Name" />
          <TableColumn fx:id="rounds" minWidth="0.0" prefWidth="90.0" text="Rounds" />
            <TableColumn fx:id="wins" minWidth="0.0" prefWidth="90.0" text="Wins" />
        </columns>
      </TableView>
      <Button fx:id="button" layoutX="392.0" layoutY="254.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="93.0" style="-fx-background-color: #d1d1d1; -fx-background-radius: 5;" text="Back">
          <font>
              <Font name="Courier New" size="14.0" />
          </font>
      </Button>
   </children>
</AnchorPane>
